<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="myEditText">
            <attr name="mytextcolor" format="color|reference"></attr>
            <attr name="mytext" format="string|reference"></attr>
    </declare-styleable>

    <declare-styleable name="myRelativeLayout">
        <attr name="myBackText" format="string|reference"></attr>
        <attr name="myBackImageID" format="integer|reference"></attr>
        <attr name="myNextText" format="string|reference"></attr>

        <attr name="myBackID" format="integer|reference"></attr>
        <attr name="myNextID" format="integer|reference"></attr>

        <attr name="myNextImageID" format="integer|reference"></attr>
        <attr name="myCaption" format="string|reference"></attr>
        <attr name="myCaptionSize" format="integer|reference"></attr>
        <attr name="myCaptionheight" format="integer|reference"></attr>
        <attr name="myCaptionLittle" format="string|reference"></attr>
        <attr name="myCaptionLittleSize" format="integer|reference"></attr>
        <attr name="myCaptionLittleheight" format="integer|reference"></attr>

    </declare-styleable>
</resources>